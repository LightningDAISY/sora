# resty redis
sudo /usr/local/openresty/bin/luarocks install luarestyredis

# template
sudo /usr/local/openresty/bin/luarocks install template

# FS
sudo /usr/local/openresty/bin/luarocks install luafilesystem

# Regex
sudo /usr/local/openresty/bin/luarocks install lrexlib-pcre

# if not exits libpcre.o & libpcre.so
wget https://ftp.pcre.org/pub/pcre/pcre-8.43.tar.gz
tar zxvf pcre-8.43.tar.gz
cd pcre-8.43
./configure --enable-utf
make
sudo make install
